<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="cowlby_duo_security_duo_verify" pattern="/duo_verify">
        <default key="_controller">cowlby_duo_security.controller.authentication:duoVerifyAction</default>
    </route>
    <route id="cowlby_duo_security_duo_test" pattern="/duo_test">
        <default key="_controller">cowlby_duo_security.controller.authentication:duoTestAction</default>
    </route>
</routes>
